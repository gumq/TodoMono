{
  "name": "TodoMono",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "homepage": "https://gumq.github.io/TodoMono/",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "start:web": "tsc -p tsconfig.web.json --noEmit && webpack serve --config web/webpack.config.js",
    "web": "webpack serve --config web/webpack.config.js --open",
    "build:web": "webpack --config web/webpack.config.js --mode production",
    "desktop": "cross-env START_URL=file://$PWD/dist/web/index.html electron .",
    "desktop:dev": "cross-env START_URL=http://localhost:8080 electron .",
    "predeploy": "yarn build:web",
    "deploy": "gh-pages -d dist/web",
    "windows": "react-native run-windows"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "^4.0.2",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-native-web": "^0.21.1",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "eslint": "^8.19.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.6.3",
    "metro-config": "^0.80.12",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
